{
    "name": "hypervel/framework",
    "type": "library",
    "description": "The Hypervel framework.",
    "license": "MIT",
    "keywords": [
        "php",
        "laravel",
        "hyperf",
        "hypervel",
        "framework",
        "swoole"
    ],
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@hypervel.org"
        }
    ],
    "support": {
        "issues": "https://github.com/hypervel/components/issues",
        "source": "https://github.com/hypervel/components"
    },
    "require": {
        "php": ">=8.2",
        "ext-hash": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "composer-runtime-api": "^2.2",
        "hyperf/database-sqlite": "3.2.x-dev",
        "hyperf/memory": "3.2.x-dev",
        "hyperf/process": "3.2.x-dev",
        "hyperf/signal": "3.2.x-dev",
        "hyperf/view-engine": "3.2.x-dev",
        "league/flysystem": "^3.25.1",
        "hypervel/auth": "dev-edge-hyperf3.2",
        "hypervel/broadcasting": "dev-edge-hyperf3.2",
        "hypervel/bus": "dev-edge-hyperf3.2",
        "hypervel/cache": "dev-edge-hyperf3.2",
        "hypervel/config": "dev-edge-hyperf3.2",
        "hypervel/console": "dev-edge-hyperf3.2",
        "hypervel/container": "dev-edge-hyperf3.2",
        "hypervel/cookie": "dev-edge-hyperf3.2",
        "hypervel/core": "dev-edge-hyperf3.2",
        "hypervel/coroutine": "dev-edge-hyperf3.2",
        "hypervel/dispatcher": "dev-edge-hyperf3.2",
        "hypervel/encryption": "dev-edge-hyperf3.2",
        "hypervel/event": "dev-edge-hyperf3.2",
        "hypervel/filesystem": "dev-edge-hyperf3.2",
        "hypervel/foundation": "dev-edge-hyperf3.2",
        "hypervel/hashing": "dev-edge-hyperf3.2",
        "hypervel/http": "dev-edge-hyperf3.2",
        "hypervel/http-client": "dev-edge-hyperf3.2",
        "hypervel/jwt": "dev-edge-hyperf3.2",
        "hypervel/log": "dev-edge-hyperf3.2",
        "hypervel/mail": "dev-edge-hyperf3.2",
        "hypervel/notifications": "dev-edge-hyperf3.2",
        "hypervel/object-pool": "dev-edge-hyperf3.2",
        "hypervel/process": "dev-edge-hyperf3.2",
        "hypervel/prompts": "dev-edge-hyperf3.2",
        "hypervel/queue": "dev-edge-hyperf3.2",
        "hypervel/router": "dev-edge-hyperf3.2",
        "hypervel/session": "dev-edge-hyperf3.2",
        "hypervel/support": "dev-edge-hyperf3.2",
        "hypervel/translation": "dev-edge-hyperf3.2",
        "hypervel/validation": "dev-edge-hyperf3.2",
        "guzzlehttp/guzzle": "^7.8.2",
        "nunomaduro/termwind": "^2.0",
        "friendsofhyperf/command-signals": "3.2.x-dev",
        "hypervel/laminas-mime": "^0.1.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.2-dev"
        }
    },
    "minimum-stability": "dev"
}
